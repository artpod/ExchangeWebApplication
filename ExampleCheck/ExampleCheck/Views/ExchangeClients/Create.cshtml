@model ExampleCheck.ExchangeClient

@{
    ViewData["Title"] = "Створити";
}

<h1>Створити</h1>

<h4>Клієнт біржі</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Exchange" class="control-label"></label>
                <select asp-for="Exchange" class ="form-control" asp-items="ViewBag.Exchange"></select>
            </div>
            <div class="form-group">
                <label asp-for="ExchangeClient1" class="control-label"></label>
                <select asp-for="ExchangeClient1" class ="form-control" asp-items="ViewBag.ExchangeClient1"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-route-id="Id">Повернутись назад</a>
</div>

